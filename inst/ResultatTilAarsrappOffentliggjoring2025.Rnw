                               KERR$Diagnose_Klinisk_Dato, units = 'days')

InklusjonKERR <- Inklusjon |>
  select(Diagnose_ny_30,InklusjonDato,
         SkjemaGUID, Diag_gr_nr, Sykehusnavn,
         UnitId, PasientGUID, Diagnose_Klinisk_Dato) |>
  filter(Diag_gr_nr==1) |>
  merge(KERR[, c("HovedskjemaGUID", "KerrsKriterier_Dato",
                 "tid_diag_kerr", "Sykdomsvurdering")],
        by.x = c("SkjemaGUID"),
        by.y = c("HovedskjemaGUID"),
        all.x = TRUE) |>
  filter(KerrsKriterier_Dato >= Diagnose_Klinisk_Dato + months(6) -
           lubridate::weeks(4),
         KerrsKriterier_Dato <= Diagnose_Klinisk_Dato + months(6) +
           lubridate::weeks(4)) |>
  mutate(Aar = lubridate::year(Diagnose_Klinisk_Dato)) |>
  summarise(Teller = as.numeric(2 %in% Sykdomsvurdering),
            N = n(),
            Sykehusnavn = first(Sykehusnavn),
            .by = c(PasientGUID, Aar, UnitId)) |>
  mutate(Aar <= rap_aar) |>
  mutate(Nevner = 1,
         ind_id = "norvas_storkar_remisjon")

ind6_AndelStorkar_remisjon_NorVas <- InklusjonKERR |>
  select(UnitId, Aar, Teller, Sykehusnavn, Nevner, ind_id)

Indikatorer <- ind6_AndelStorkar_remisjon_NorVas |>
  rename(year = Aar,
         var = Teller,
         denominator = Nevner) |>
  select(UnitId, year, var, denominator, ind_id) |>
  bind_rows(Indikatorer)

tabell_remisjon_6mnd <- ind6_AndelStorkar_remisjon_NorVas |>
  filter(Aar <= rap_aar) |>
  group_by(Sykehusnavn) |>
  summarise(Antall = sum(Teller),
            N = n()) |>
  janitor::adorn_totals() |>
  mutate(Andel = Antall/N*100) |>
  select(Sykehusnavn, Andel, N)

tabell_remisjon_6mnd_aar <- ind6_AndelStorkar_remisjon_NorVas |>
  filter(Aar <= rap_aar,
         Aar > rap_aar-5) |>
  summarise(Antall = sum(Teller),
            N = n(),
            .by = c(Sykehusnavn, Aar)) |>
  group_by(Aar) |>
  group_modify(~ .x |> janitor::adorn_totals()) |>
  # group_by(Sykehusnavn) |>
  # group_modify(~ .x |> janitor::adorn_totals()) |>
  mutate(Andel = Antall/N*100,
         Andel = paste0(sprintf("%.1f", Andel), "% (", N, ")"),
         Andel = ifelse(N<5, "N<5", Andel)) |>
  select(Sykehusnavn, Aar, Andel) |>
  tidyr::pivot_wider(names_from = Aar, values_from = Andel, names_sort = TRUE) |>
  arrange(Sykehusnavn)

