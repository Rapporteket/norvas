aux <- Inklusjon |>
  select(PasientGUID) |>
  merge(Oppfolging |> filter(EksklusjonsArsak != -1) |>
          select(PasientGUID, EksklusjonsArsak, EksklusjonsDato),
        by = "PasientGUID", all.x = T) |>
  mutate(eksklusjonsaar = as.numeric(format(EksklusjonsDato, format="%Y"))) |>
  mutate(eksklusjonsaar = ifelse(is.na(eksklusjonsaar), 2050, eksklusjonsaar))

diagnosefordeling <- Diagnoser |>
  merge(aux, by = "PasientGUID") |>
  filter(Diagnose_Klinisk_Dato < paste0(rap_aar+1, "-01-01")) |>
  summarise(ant_rapaar = sum(eksklusjonsaar >= rap_aar),
            ant_foer_rapaar = sum(eksklusjonsaar < rap_aar),
            N = n(),
            .by = "Diagnose") |>
  arrange(N)

plotMatrise <- t(as.matrix(diagnosefordeling[,c(2,3)]))
colnames(plotMatrise) <- rep('', dim(plotMatrise)[2])
outfile <- paste0(figfolder, 'diagnosefordeling_splittet.', utformat)
grtxt <- diagnosefordeling$Diagnose |> norvas::wrap.it(28)
legendTxt <- c(paste0('Følges opp i ', rap_aar, ', N=', sum(diagnosefordeling$ant_rapaar, na.rm = T)),
               paste0('Ekskludert før ', rap_aar, ', N=', sum(diagnosefordeling$ant_foer_rapaar, na.rm = T)))
norvarStabelGrvar(plotMatrise=plotMatrise/sum(diagnosefordeling$N)*100,
                  grtxt = grtxt,
                  outfile = outfile,
                  tittel = "Diagnosefordeling",
                  xlab =" Andel pasienter (%)",
                  legendTxt = legendTxt,
                  pstTxt = paste0(round(diagnosefordeling$N/sum(diagnosefordeling$N)*100, 1), "%"),
                  NTxt = diagnosefordeling$N)

