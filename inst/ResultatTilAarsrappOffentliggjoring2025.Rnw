############### Andel på prednisolondose mindre enn 5 etter 6 mnd  ######

prednisolon_6mnd_v2 <- Inklusjon |>
  select(Diagnose_ny_30,InklusjonDato, SkjemaGUID,
         Diag_gr_nr, Sykehusnavn, UnitId, PasientGUID,
         Diagnose_Klinisk_Dato) |>
  filter(Diag_gr_nr==2) |>
  mutate(
    Ved_start_int = lubridate::interval(
      Diagnose_Klinisk_Dato - lubridate::weeks(4),
      Diagnose_Klinisk_Dato + lubridate::weeks(4)),
    Ved_6mnd_int = lubridate::interval(
      Diagnose_Klinisk_Dato %m+% months(6) - lubridate::weeks(4),
      Diagnose_Klinisk_Dato %m+% months(6) + lubridate::weeks(4))) |>
  merge(Medisiner |> filter(LegemiddelGenerisk == "Prednisolon") |>
          select(HovedskjemaGUID, Med_StartDato,
                 Med_SluttDato, Dose),
        by.x = "SkjemaGUID", by.y = "HovedskjemaGUID") |>
  dplyr::filter(Med_StartDato %within% Ved_6mnd_int) |>
  dplyr::filter(abs(difftime(Med_StartDato,
                             Diagnose_Klinisk_Dato %m+% months(6))) ==
                  min(abs(difftime(Med_StartDato,
                                   Diagnose_Klinisk_Dato %m+% months(6)))),
                .by = PasientGUID)|>
  mutate(Teller = as.numeric(Dose <= 5),
         Nevner = 1,
         Aar = lubridate::year(Diagnose_Klinisk_Dato),
         ind_id = "norvas_anca_prednisolon")


