<<'Indikator 8 og 9: pas med anca med årlig måling av Igg', results='asis', echo=FALSE, warning=F>>=

## inklusjonsskjema for alle anca-pasienter. var = 1 for alle som får
## målt igg ved inklusjon
inkl_anca_igg <- Inklusjon |>
  filter(Diag_gr_nr == 2) |>
  select(PasientGUID, Sykehusnavn, UnitId, InklusjonDato) |>
  merge(Labskjema |> select(PasientGUID, Blodprover_Dato, IgGVerdi),
        by.x = c('PasientGUID','InklusjonDato'),
        by.y = c('PasientGUID', 'Blodprover_Dato'), all.x = TRUE) |>
  mutate(var = as.numeric(!is.na(IgGVerdi)),
         denominator = 1,
         year = lubridate::year(InklusjonDato),
         ind_id = "norvas_aarlig_igg_anca") |>
  select(PasientGUID, UnitId, Sykehusnavn, year, var, denominator, ind_id) |>
  filter(year <= rap_aar)

## Inneholder alle anca-pasienter som har et oppfølgingsskjema et gitt år etter
## inklusjon og før en ev. eksklusjon. var = 1 for alle som får
## målt igg ved oppfølging
oppf_anca_igg <- Oppfolging |>
  filter(Diag_gr_nr == 2) |>
  select(PasientGUID, Sykehusnavn, UnitId, OppfolgingsDato, InklusjonDato) |>
  merge(Oppfolging |> dplyr::select(PasientGUID, EksklusjonsDato) |>
          dplyr::filter(!is.na(EksklusjonsDato)),
        by = "PasientGUID", all.x = TRUE) |>
  filter(lubridate::year(OppfolgingsDato) > lubridate::year(InklusjonDato)) |>
  merge(Labskjema |> select(PasientGUID, Blodprover_Dato, IgGVerdi),
        by.x = c('PasientGUID','OppfolgingsDato'),
        by.y = c('PasientGUID', 'Blodprover_Dato'), all.x = TRUE) |>
  mutate(var = as.numeric(!is.na(IgGVerdi)),
         Eksklusjonsaar = lubridate::year(EksklusjonsDato),
         Eksklusjonsaar = ifelse(is.na(Eksklusjonsaar), 2050,
                                 Eksklusjonsaar),
         year = lubridate::year(OppfolgingsDato)) |>
  filter(lubridate::year(OppfolgingsDato) < Eksklusjonsaar) |>
  summarise(var = max(var),
            Sykehusnavn = first(Sykehusnavn),
            .by = c(PasientGUID, year, UnitId)) |>
  mutate(denominator = 1,
         ind_id = "norvas_aarlig_igg_anca") |>
  select(PasientGUID, UnitId, Sykehusnavn, year, var, denominator, ind_id) |>
  filter(year <= rap_aar)



aktuelle_anca <- Inklusjon |>
  filter(Diag_gr_nr == 2) |>
  select(UnitId, Sykehusnavn, PasientGUID, Inklusjonsaar, InklusjonDato) |>
  as_tibble()
aux <- aktuelle_anca[1, ]

# For alle pasienter, lag en rad for hvert år pasienten har vært inkludert
# som inneholder UnitId, pasientguid, inklusjonsår og år for potensiell oppfølging
for (id in aktuelle_anca$PasientGUID) {
  for (aar in aktuelle_anca$Inklusjonsaar[
    aktuelle_anca$PasientGUID==id]:rap_aar) {
    aux <- bind_rows(
      aux,
      bind_cols(aktuelle_anca[aktuelle_anca$PasientGUID==id, ],
                Oppfolgingsaar=aar))
  }
}

## Inneholder alle anca-pasienter som er inkludert minst året før og som ennå
## ikke er ekskludert. var = 1 for alle som får målt igg ved oppfølging
oppf_anca_igg_alle <- aux[-1, ] |>
  merge(Oppfolging |> dplyr::select(PasientGUID, EksklusjonsDato) |>
          dplyr::filter(!is.na(EksklusjonsDato)),
        by = "PasientGUID", all.x = TRUE) |>
  dplyr::mutate(Eksklusjonsaar = lubridate::year(EksklusjonsDato),
                Eksklusjonsaar = ifelse(is.na(Eksklusjonsaar), 2050,
                                        Eksklusjonsaar)) |>
  dplyr::filter(Oppfolgingsaar > Inklusjonsaar,
                Oppfolgingsaar < Eksklusjonsaar) |>
  rename(year = Oppfolgingsaar) |>
  merge(oppf_anca_igg |> select(PasientGUID, UnitId, year, var),
        by = c("PasientGUID", "UnitId", "year"),
        all.x = TRUE) |>
  mutate(var = ifelse(is.na(var), 0, var)) |>
  filter(year <= rap_aar)



tab_igg_maalt_anca_inkl <- inkl_anca_igg |>
  filter(year <= rap_aar,
         year > rap_aar-5) |>
  summarise(Antall = sum(var),
            N = n(),
            .by = c(Sykehusnavn, year)) |>
  group_by(year) |>
  group_modify(~ .x |> janitor::adorn_totals()) |>
  # group_by(Sykehusnavn) |>
  # group_modify(~ .x |> janitor::adorn_totals()) |>
  mutate(Andel = Antall/N*100,
         Andel = paste0(sprintf("%.1f", Andel), "% (", N, ")"),
         Andel = ifelse(N<5, "N<5", Andel)) |>
  select(Sykehusnavn, year, Andel) |>
  tidyr::pivot_wider(names_from = year, values_from = Andel,
                     names_sort = TRUE) |>
  arrange(Sykehusnavn)


