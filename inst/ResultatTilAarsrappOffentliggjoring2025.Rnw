Alder_v_diagnose <- RegData |>
  dplyr::filter(lubridate::year(Diagnose_Klinisk_Dato) <= rap_aar ) |>
  dplyr::mutate(Alder = cut(DiagnoseAlder, breaks = c(seq(16,76,by=10), 140),
                            include.lowest = T, right = F)) |>
  dplyr::count(Diagnose, Alder) |>
  dplyr::group_by(Diagnose) |>
  dplyr::group_modify(~ .x |> janitor::adorn_totals()) |>
  tidyr::pivot_wider(names_from = Alder, values_from = n, values_fill = 0) |>
  dplyr::mutate(dplyr::across(1:7, ~ ./ Total * 100)) |>
  dplyr::mutate(dplyr::across(1:7, ~ paste0(sprintf("%.1f", .), " %"))) |>
  dplyr::rename(N = Total)

