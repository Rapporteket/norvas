                         by.x = c('PasientGUID','OppfolgingsDato'),
                         by.y = c('PasientGUID', 'Blodprover_Dato'), all.x = T)

andel_utfort_igG <- oppfolging_anca %>%
  group_by(Sykehusnavn) %>%
  summarise(ant_igG = sum(!is.na(IgGVerdi)),
            N = n(),
            andel_igG = ant_igG/N*100)

